<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamB.controllers.EquipmentRequestController">
   <children>
      <Label layoutX="152.0" layoutY="48.0" text="Medical Equipment Request">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="284.0" layoutY="92.0" text="Room" />
      <ComboBox fx:id="roomComboBox" layoutX="222.0" layoutY="110.0" onAction="#setRoom" prefWidth="150.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:id="ROOM_1" fx:value="Room 1" />
               <String fx:id="ROOM_2" fx:value="Room 2" />
               <String fx:id="ROOM_3" fx:value="Room 3" />
            </FXCollections>
         </items>
      </ComboBox>
      <Label alignment="CENTER" layoutX="264.0" layoutY="150.0" prefHeight="18.0" prefWidth="66.0" text="Equipment" />
      <ComboBox fx:id="equipmentComboBox" layoutX="222.0" layoutY="169.0" onAction="#setEquipment" prefWidth="150.0">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:id="BED" fx:value="Bed" />
               <String fx:id="RECLINER" fx:value="Recliner" />
               <String fx:id="XRAY_MACHINE" fx:value="X-Ray Machine" />
               <String fx:id="INFUSION_PUMP" fx:value="Infusion Pump" />
            </FXCollections>
         </items>
      </ComboBox>
      <Label fx:id="resultLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="319.0" prefHeight="18.0" prefWidth="547.0" />
      <Button layoutX="253.0" layoutY="270.0" mnemonicParsing="false" onAction="#sendRequest" text="Send Request" />
      <Button layoutX="18.0" layoutY="360.0" mnemonicParsing="false" onAction="#goToHomepage" text="Back to Homepage" />
      <Button layoutX="527.0" layoutY="360.0" mnemonicParsing="false" onAction="#reset" text="Reset" />
      <TextField fx:id="nameInput" layoutX="222.0" layoutY="225.0" prefHeight="26.0" prefWidth="150.0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="256.0" layoutY="206.0" prefHeight="18.0" prefWidth="89.0" text="Staff Member" textAlignment="CENTER" />
      <MenuBar fx:id="homeBar" layoutY="1.0" prefHeight="25.0" prefWidth="600.0">
         <menus>
            <Menu mnemonicParsing="false" text="Profile">
               <items>
                  <MenuItem mnemonicParsing="false" text="Log In" />
                  <MenuItem mnemonicParsing="false" text="View Profile" />
                  <MenuItem mnemonicParsing="false" onAction="#goToHomepage" text="Homepage" />
                  <MenuItem mnemonicParsing="false" onAction="#quitApplication" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Map">
               <items>
                  <MenuItem mnemonicParsing="false" text="View" />
                  <MenuItem mnemonicParsing="false" text="Edit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Equipment Tracker" />
            <Menu mnemonicParsing="false" text="Requests">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#goToEquipmentDelivery" text="Request Medical Equipment Delivery" />
                  <MenuItem mnemonicParsing="false" onAction="#goToLabService" text="Request Lab Service" />
                  <MenuItem mnemonicParsing="false" onAction="#goToMedicineDelivery" text="Request Medicine Delivery" />
                  <MenuItem mnemonicParsing="false" onAction="#goToMealDelivery" text="Request Patient Meal Delivery" />
                  <MenuItem mnemonicParsing="false" onAction="#goToInterpreterPage" text="Request Language Interpreter" />
                  <MenuItem mnemonicParsing="false" onAction="#goToInternalPatient" text="Request Internal Patient Transfer" />
               </items>
            </Menu>
            <Menu disable="true" mnemonicParsing="false" text="Patient Database" />
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
                  <MenuItem mnemonicParsing="false" text="à¶ž" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
